<span class="n">d</span><span class="o">.</span><span class="n">ix</span><span class="p">[</span><span class="s">&#39;testA&#39;</span><span class="p">,</span><span class="s">&#39;lin_reg&#39;</span><span class="p">]</span> <span class="o">=</span> <span class="n">get_err</span><span class="p">(</span><span class="n">Y_test</span><span class="p">,</span><span class="n">X_test</span><span class="p">,</span><span class="n">beta_linreg</span><span class="p">,</span><span class="mf">0.5</span><span class="p">)</span>
</pre></div>

